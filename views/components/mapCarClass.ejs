<div class="map-car-class">
    <b style="padding: 2px 10px; border-radius: 5px; background-color: white; color: var(--primary)"><%= obj.name %></b>
    <% for (let i = 0; i < obj.cars.length; i++) { 
        const route = lang == "ENG" ? "fleet/" : "flota/";
        const url = "./" + route + obj.cars[i].car_slug;
    %>
        <p>
            <a href="<%= url %>"><%= obj.cars[i].car_title %></a>
        </p>
    <% } %>
</div>