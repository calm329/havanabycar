<% 

const options = [];
const hours = [];
const minutes = ['00', '30'];
for(let i = 0; i < 24; i++) {
    const item = i < 10 ? '0' + i : i;
    hours.push(item);
}

for(let i = 0; i < hours.length; i++){
    minutes.forEach(item => options.push(hours[i] + ':' + item));
}

%>

<% for(let i = 0; i < options.length; i++){ %>
    <div>
        <p value = "<%= options[i] %>">
            <%= options[i] %>
        </p>
    </div>
<% } %>