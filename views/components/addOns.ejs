<div class="add-ons">
    <div>
        <% if(req.params.city != req.params.dropoffCity ) { %>
            <p><i class="fas fa-check-circle"></i> <%= footer.intercity %> (35 <span class="currencySymbol"></span>)</p>
        <% } %>
        <% if(req.params.city.includes('Airport') ) { %>
            <p><i class="fas fa-check-circle"></i> <%= footer.airportFee %> (35 <span class="currencySymbol"></span>)</p>
        <% } %>
        <p><i class="fas fa-check-circle"></i> <%= footer.insurance %></p>
        <p><i class="fas fa-check-circle"></i> <%= footer.fuel %> (60 <span class="currencySymbol"></span>)</p>

        <p><i class="fas fa-check-circle"></i> <%= footer.unlimited %></p>
    </div>
</div>